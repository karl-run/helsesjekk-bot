{
    "name": "helsesjekk",
    "version": "1.0.0",
    "main": "dist/index.js",
    "license": "MIT",
    "packageManager": "yarn@3.3.1",
    "scripts": {
        "build": "tsc",
        "dev": "nodemon src/index.ts | pino-pretty",
        "lint": "eslint src/**/*.ts",
        "prisma:generate": "prisma generate"
    },
    "dependencies": {
        "@prisma/client": "4.9.0",
        "@slack/bolt": "^3.12.2",
        "dotenv": "^16.0.3",
        "node-cron": "^3.0.2",
        "pino": "^8.8.0",
        "prisma": "^4.9.0",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@navikt/eslint-config-teamsykmelding": "^1.4.0",
        "@types/node": "^18.11.18",
        "@types/node-cron": "^3.0.7",
        "@types/uuid": "^9.0.0",
        "@typescript-eslint/eslint-plugin": "^5.49.0",
        "@typescript-eslint/parser": "^5.49.0",
        "@vercel/ncc": "^0.36.0",
        "eslint": "^8.32.0",
        "eslint-plugin-import": "^2.27.5",
        "nodemon": "^2.0.20",
        "pino-pretty": "^9.1.1",
        "prettier": "^2.8.3",
        "prettier-plugin-prisma": "^4.9.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4"
    },
    "prettier": "@navikt/eslint-config-teamsykmelding/prettier",
    "eslintConfig": {
        "root": true,
        "extends": [
            "eslint:recommended",
            "plugin:@typescript-eslint/recommended"
        ],
        "parser": "@typescript-eslint/parser",
        "plugins": [
            "@typescript-eslint",
            "import"
        ],
        "rules": {
            "import/order": [
                "warn",
                {
                    "newlines-between": "always"
                }
            ]
        }
    }
}
